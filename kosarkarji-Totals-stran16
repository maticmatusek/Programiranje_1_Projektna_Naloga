url = url.replace("&"+pos+"=&","&");
url = url.replace("?"+pos+"=","");
url = url.replace("&"+pos+"=","");
url = url.replace("&"+pos,"");
} else {
console.log("index is not found");
if (url.indexOf("?") >= 0) {
url = url+"&"+pos+"=";
} else {
url = url+"?"+pos+"=";
}
}
console.log("url is now: "+url);
open_network(url);
}
